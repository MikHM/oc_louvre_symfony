{% extends "@Booking/layout.html.twig" %}

{% block head %}
    {{ parent() }}
    {%  block title %}Billeterie{% endblock %}
{% endblock %}

{% block body %}

<div class="row">
    <div class="container">
        <div class="row">
            {#{{ form(form, {attr: {novalidate: "novalidate"}}) }}#}

            {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
            <div>
                {{ form_label(form.dateOfVisit) }}
                <br>
                {{ form_errors(form.dateOfVisit) }}
                {{ form_widget(form.dateOfVisit) }}
            </div>
            {#{{ form_widget(form) }}#}
            {{ form_end(form) }}

        </div>
    </div>
</div>

{% endblock %}